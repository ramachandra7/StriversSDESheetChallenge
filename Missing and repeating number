#include <bits/stdc++.h>

pair<int,int> missingAndRepeating(vector<int> &arr, int n)
{
	// Write your code here \
	// s - sn = x - y
	// s2 - s2n =  x2 - y2

long long int i,s=0,s2=0,sn=0,s2n=0,val1,val2;

	for(i=0;i<n;i++){
		s+=arr[i];
		s2+=((long long)arr[i]* (long long)arr[i]);
		sn+=(i+1);
		s2n+=(i+1)*(i+1);
	}
	val1=s-sn;
	val2=s2-s2n;
	val2/=val1;
int x=(int)(val1+val2)/2;
	return {x-val1,x};


	
}
